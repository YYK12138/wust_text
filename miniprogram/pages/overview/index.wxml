<!--miniprogram/pages/overview/index.wxml-->
<swiper
  indicator-dots="{{indicatorDots}}"
  autoplay="{{autoplay}}"
  interval="{{interval}}"
  duration="{{duration}}">
  <block wx:for="{{imgUrls}}">
    <swiper-item>
      <image src="{{item}}" class='img'/>
    </swiper-item>
  </block>
</swiper>
<view class='container1'>
 <view class='container2'>
  <button class='button1' bindtap='test1'><text >{{name1}}</text></button>
 </view>

 <view class='container3'>
  <text>{{open1}}</text>
  <text> 容量:{{num1}}</text>
  <text space='ensp'>剩余:{{margin1}}</text>
 </view>

 <view class='container2'>
  <button class='button1' bindtap='test2' ><text>{{name2}}</text></button>
 </view>

 <view class='container3'>
  <text>{{open2}}</text>
  <text> 容量:{{num2}}</text>
  <text space='ensp'> 剩余:{{margin2}}</text>
 </view>

 <view class='container2'>
  <button class='button1' bindtap='test3' ><text>{{name3}}</text></button>
 </view>

 <view class='container3'>
  <text>{{open3}}</text>
  <text>容量:{{num3}}</text>
  <text space='ensp'>剩余:{{margin3}}</text>
  </view>
</view>